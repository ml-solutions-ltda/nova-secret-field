{
    "name": "mlsolutions/nova-secret-field",
    "description": "This package provides a custom secret field that can be toggled to see its value.",
    "keywords": [
        "laravel",
        "nova",
        "laravel-nova",
        "laravel-nova-field",
        "nova-fields",
        "nova-4"
    ],
    "homepage": "https://github.com/ml-solutions-ltda/nova-secret-field",
    "support": {
        "issues": "https://github.com/ml-solutions-ltda/nova-secret-field/issues",
        "source": "https://github.com/ml-solutions-ltda/nova-secret-field",
        "docs": "https://github.com/ml-solutions-ltda/nova-secret-field#readme"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Norman Huth",
            "homepage": "https://huth.it"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/nova": "^5.0.0"
    },
    "autoload": {
        "psr-4": {
            "MlSolutions\\SecretField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MlSolutions\\SecretField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
